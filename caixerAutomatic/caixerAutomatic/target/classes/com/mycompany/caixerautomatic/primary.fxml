<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.caixerautomatic.PrimaryController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <Pane prefHeight="600.0" prefWidth="600.0">
         <children>
            <Label fx:id="titleLabel" layoutX="14.0" layoutY="14.0" text="Login">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <Label fx:id="usernameLabel" layoutX="14.0" layoutY="72.0" text="Username" />
            <Label fx:id="passwordLabel" layoutX="14.0" layoutY="132.0" text="Password" />
            <TextField fx:id="usernameField" layoutX="14.0" layoutY="99.0" />
            <TextField fx:id="passwordField" layoutX="14.0" layoutY="155.0" />
            <Button fx:id="loginButton" layoutX="16.0" layoutY="204.0" mnemonicParsing="false" text="Login" />
         </children>
      </Pane>
   </children>
</VBox>
